<ul class="media-list">
  <% @favos.each do |favo| %>
    <li class="media">
      <div class="media-body">
        <div>
          <span class="text-muted">posted at <%= favo.created_at %></span>
        </div>
        <div>
          <p><%= favo.content %></p>
        </div>
      </div>
    </li>
  <% end %>
  <%= paginate @favos %>
</ul>